CREATE TABLE `role` (   `rid` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Primary Key: Unique role ID.',   `name` varchar(64) NOT NULL DEFAULT '' COMMENT 'Unique role name.',   `weight` int(11) NOT NULL DEFAULT '0' COMMENT 'The weight of this role in listings and the user interface.',   PRIMARY KEY (`rid`),   UNIQUE KEY `name` (`name`),   KEY `name_weight` (`name`,`weight`) ) AUTO_INCREMENT=4;
